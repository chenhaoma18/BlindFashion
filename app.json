{
  "expo": {
    "name": "Blind Fashion",
    "slug": "BlindFashion",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/RecentLogo.png",
    "splash": {
      "image": "./assets/images/whitesplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.changeplusplus.kumiapp",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "infoPlist": {
        "NFCReaderUsageDescriptionKey" : "This app uses the NFC reader to read information on tags that are attached to clothing items to get a list of attributes associated with that clothing item such as color, size, material, etc.",
      }
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  },
  "displayName": "Kumi",
  "name": "Kumi"
}
